// Example 4: Conditional logic

var score = 85
var grade = ""

if score >= 90 then:
    grade = "A"
elseif score >= 80 then:
    grade = "B"
elseif score >= 70 then:
    grade = "C"
elseif score >= 60 then:
    grade = "D"
else:
    grade = "F"
end

print "Your grade is: " + grade

